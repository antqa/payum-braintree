{
    "name": "antqa/payum-braintree",
    "type": "library",
    "description": "The Payum extension. It provides Braintree payment integration.",
    "keywords": ["payum", "payment", "braintree", "symfony"],
    "homepage": "https://www.ant.qa",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Antosik",
            "email": "piotr.antosik@ant.qa"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/antqa/payum-braintree/contributors"
        }
    ],
    "require": {
        "php": "^7.4 | ^8.0 | ^ 8.1",
        "payum/core": "^1.7",
        "braintree/braintree_php": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": { "Antqa\\Payum\\Braintree\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
